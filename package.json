{
  "name": "nexus_hub",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev": "concurrently \"npm run db:up\" \"npm run dev:app\"",
    "db:up": "docker-compose up -d",
    "dev:app": "wait-on tcp:5432 && npm run db:migrate-seed && next dev --turbopack",
    "db:migrate-seed": "npm run db:migrate && npm run db:seed",
    "db:migrate": "prisma migrate dev --name init",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.17.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "prisma": "^6.6.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  }
}
